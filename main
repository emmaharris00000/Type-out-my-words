lines = [
    #include your scripted obejcts here, ex: "this is a line that will be read into terminal", "there will be a pause between this and the last sentence", "",
]
 
from time import sleep
import sys
 
for line in lines:
    for c in line: 
        print(c, end='')  
        sys.stdout.flush()  
        (sleep(0.05))         
    sleep(.4)
    print() 
 
 
